{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "isNew": {
      "type": "boolean"
    },
    "isOnSale": {
      "type": "boolean"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "blocks"
    },
    "price": {
      "type": "decimal"
    },
    "oldPrice": {
      "type": "decimal"
    },
    "size": {
      "type": "string"
    },
    "material": {
      "type": "string"
    },
    "density": {
      "type": "string"
    },
    "quantityInPack": {
      "type": "integer"
    },
    "stock": {
      "type": "integer"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "products"
    },
    "isBestseller": {
      "type": "boolean"
    },
    "polishes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::polish.polish"
    },
    "designers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::designer.designer"
    },
    "batch": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::batch.batch"
    }
  }
}
